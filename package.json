{
  "name": "react-ko-calendar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dayjs": "^1.11.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "spr": "yarn gulp spr",
        "spr:module": "yarn gulp spr-module",
        "generate-icon-export": "node ./src/components/common/Icon/tools/generateIcons.js",
        "generate-svgr": "cd ./src/components/common/Icon/svgs && npx @svgr/cli@5.5.0 --template ../tools/template.js --config-file ../tools/.svgrrc *.svg --out-dir ../icons --ignore-existing",
        "svgr": "yarn generate-svgr && yarn generate-icon-export"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.50",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {}
}
